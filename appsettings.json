{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "SectionUrlPage": {
    "Url": "https://www.conectate.com.do/loterias"
  },
  "XPathExpression": {
    "XPATHGeneral": "//section[@class='col-content']/div[contains(@class, 'game-block ')]",
    "XPATHTitulo": "div[@class='game-info']/div[@class='game-details']/a[@class='game-title']/span",
    "XPATHFecha": "div[@class='game-info']/div[@class='game-details']/div[@class='session-details']/span",
    "XPATHImagenes": "div[@class='game-info']/div[@class='game-logo']/img",
    "XPATHNumeros": "div[@class='game-scores ball-mode']/span",
    "XPATHNumerosEspeciales": "div[@class='game-scores ball-mode']/table/tbody/tr"
  }

}
